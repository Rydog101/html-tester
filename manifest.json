{
   "name": "html tester",
   "version": "0.1",
   "manifest_version": 2,
   "description": "Test your html code",
   "browser_action": {
		"default_icon": "icon.png",
		"default_title": "html tester",
		"default_popup": "html.html"
   },
   "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
   "permissions": [
   "tabs"
   ],
   "icons": {
		"128": "icon.png"
   }
}
